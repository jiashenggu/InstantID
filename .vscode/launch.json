    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: usage.py",
            "type": "debugpy",
            "request": "launch",
            "program": "usage.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python Debugger: app.py",
            "type": "debugpy",
            "request": "launch",
            "program": "gradio_demo/app.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Python Debugger: app_dev.py",
            "type": "debugpy",
            "request": "launch",
            "program": "gradio_demo/app_dev.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "CUDA_VISIBLE_DEVICES": "1",
            }
        }
    ]
}